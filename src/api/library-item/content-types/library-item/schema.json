{
  "kind": "collectionType",
  "collectionName": "library_items",
  "info": {
    "singularName": "library-item",
    "pluralName": "library-items",
    "displayName": "ðŸ”´ Library Items",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "subtitle": {
      "pluginOptions": {},
      "type": "string"
    },
    "slug": {
      "pluginOptions": {},
      "type": "string",
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
      "required": true,
      "unique": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "subitems": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::library-item.library-item",
      "mappedBy": "subitem_of"
    },
    "subitem_of": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::library-item.library-item",
      "inversedBy": "subitems"
    },
    "root_item": {
      "pluginOptions": {},
      "type": "boolean",
      "default": true,
      "required": true
    },
    "price": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {},
      "component": "basics.price"
    },
    "metadata": {
      "pluginOptions": {},
      "type": "dynamiczone",
      "components": [
        "metadata.audio",
        "metadata.books",
        "metadata.game",
        "metadata.group",
        "metadata.other",
        "metadata.video"
      ],
      "max": 1
    },
    "size": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {},
      "component": "basics.size"
    },
    "release_date": {
      "type": "component",
      "repeatable": false,
      "component": "basics.datepicker",
      "required": false
    },
    "descriptions": {
      "displayName": "Library Items",
      "type": "component",
      "repeatable": true,
      "component": "translations.library-items"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ranged-content.ranged-content",
      "mappedBy": "library_item"
    },
    "digital": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "primary": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "submerchs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::merch-item.merch-item",
      "mappedBy": "library_items"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "urls": {
      "displayName": "Url",
      "type": "component",
      "repeatable": true,
      "component": "basics.url"
    },
    "images": {
      "type": "component",
      "repeatable": true,
      "component": "collections-component.library-images"
    }
  }
}
