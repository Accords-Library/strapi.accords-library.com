        "wiki-specialization.weapon"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "translations": {
      "displayName": "Wiki",
      "type": "component",
      "repeatable": true,
      "component": "translations.wiki"
    }
  }
