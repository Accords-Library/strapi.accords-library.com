{
  "kind": "collectionType",
  "collectionName": "contents",
  "info": {
    "singularName": "content",
    "pluralName": "contents",
    "displayName": "ðŸŸ¡ Contents",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::content-type.content-type"
    },
    "ranged_contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ranged-content.ranged-content",
      "mappedBy": "content"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "translations": {
      "type": "component",
      "repeatable": true,
      "component": "translations.title"
    },
    "chronicles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chronicle.chronicle",
      "mappedBy": "contents"
    },
    "folder": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::contents-folder.contents-folder",
      "inversedBy": "contents"
    }
  }
}
