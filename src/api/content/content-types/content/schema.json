    "slug": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::content-type.content-type"
    },
    "titles": {
      "displayName": "title",
      "type": "component",
      "repeatable": true,
